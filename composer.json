  "type": "typo3-cms-extension",
  "keywords": ["TYPO3 CMS", "OnePager", "Gridelements"],
  "homepage": "https://forge.typo3.org/projects/extension-gridelements2",
  "license": "GPL-2.0+",
  "require": {
